<div class="mdc-card">
  <div class="mdc-card__info--content-primary">
    <div class="mdc-card__info--content-primary-title mdc-typography--headline4">
      Deleting your account&hellip;
    </div>
  </div>
</div>
<script>
  document.addEventListener("DOMContentLoaded", function(event) {
    window.setTimeout(function() {
      thirdforcenews.auth.deleteUser().then(function() {
        window.location.href = '/';
      }).catch(function(err) {
        console.error('Failed to delete user', err);
      });
    }, 1000);
  });
</script>
